<!DOCTYPE HTML>
<html lang="en">
<head>
  <meta charset="utf-8" />
  <title>Express yourself</title>
</head>
<body>
  <form method="POST" action="/names">
    <input type="text" name="name" />
    <input type="submit" value="submit" />
  </form>

  <ul>
    <% Object.keys(names).forEach(function(name) { %>
      <li><%= name %> <%= names[name] %></li>
  <% }) %>
  </ul>

</body>
</html>
